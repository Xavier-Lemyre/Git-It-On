body { /* Normal  */
  font-size: 24px;
  font-family: "Cambria", sans-serif;
  line-height: 25px ;
  text-align: justify;
}
td {  /* Table  */
  font-size: 20px;
  font-family: "Cambria", sans-serif;
  line-height: 22px;
  text-align: center;
}
table {
  padding-left: 10px;
  padding-right: 10px;
  min-width: 75%;
  text-align:center;
  margin: 0 auto;
  }
.title {
  font-size: 22px;
  font-family: "Cambria", sans-serif;
  font-weight: bold;
  font-variant: small-caps;
  text-align: center;
  padding: 96px;
}
h1 { /* Header 1 */
  font-size: 26px;
  font-family: "Cambria", sans-serif;
  font-weight: bold;
  font-variant: small-caps;
  text-align: center;
}
h2 { /* Header 2 */
  font-size: 26px;
  font-family: "Cambria", sans-serif;
  font-variant: small-caps;
  font-weight: bold;
  text-align: center;
}
h3 { /* Header 3 */
  font-size: 24px;
  font-family: "Cambria", sans-serif;
  font-variant: small-caps;
  font-weight: bold;
  text-align: left;
}
h4 { /* Header 4 */
  font-size: 23px;
  font-family: "Cambria", sans-serif;
  font-variant: small-caps;
  text-align: left;
}
ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: #333;
  position: fixed;
  top: 0;
  width: 100%;
}

li {
  float: left;
}

li a {
  display: block;
  color: white;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
}

li a:hover:not(.active) {
  background-color: #111;
}

.active {
  background-color: #4CAF50;
}
.column {
  float: left;
  padding: 10px;
}
.left, .right {
  width: 45%;
}
.middle {
  width: 10%;
}
.row:after {
  content: "";
  display: table;
  clear: both;
}
.dropbtn {
  background-color: #4CAF50;
  color: white;
  padding: 16px;
  font-size: 16px;
  border: none;
  cursor: pointer;
}
.dropdown {
  position: relative;
  display: inline-block;
}
.dropdown-content {
  display: none;
  position: absolute;
  background-color: #f9f9f9;
  min-width: 160px;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
  z-index: 1;
}
.dropdown-content a {
  color: black;
  padding: 12px 16px;
  text-decoration: none;
  display: block;
}
.dropdown-content a:hover {background-color: #f1f1f1}
.dropdown:hover .dropdown-content {
  display: block;
}
.dropdown:hover .dropbtn {
  background-color: #3e8e41;
}
</style>
